  "components": {
    "schemas": {
      "auth.body": {
        "type": "object",
        "properties": {
          "username": {
            "minLength": 3,
            "maxLength": 31,
            "pattern": "[a-z0-9_-]+",
            "type": "string"
          },
          "password": {
            "minLength": 6,
            "maxLength": 255,
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "auth.response": {
        "type": "object",
        "properties": {
          "tokenType": {
            "type": "string"
          },
          "session": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "userId": {
                "type": "string"
              },
              "fresh": {
                "type": "boolean"
              },
              "expiresAt": {
                "anyOf": [
                  {
                    "type": "Date"
                  },
                  {
                    "format": "date",
                    "default": "2024-04-19T10:22:45.207Z",
                    "type": "string"
                  },
                  {
                    "format": "date-time",
                    "default": "2024-04-19T10:22:45.207Z",
                    "type": "string"
                  }
                ]
              }
            },
            "required": [
              "id",
              "userId",
              "fresh",
              "expiresAt"
            ]
          },
          "username": {
            "type": "string"
          }
        },
        "required": [
          "tokenType",
          "session",
          "username"
        ]
      },
      "matrices": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "name": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "name"
          ]
        }
      },
      "matrix": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "choices": {
            "type": "object",
            "properties": {
              "list": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "list"
            ]
          },
          "factors": {
            "type": "object",
            "properties": {
              "list": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "list"
            ]
          },
          "factorsChoices": {
            "type": "object",
            "properties": {
              "matrix": {
                "type": "array",
                "items": {
                  "type": "array",
                  "items": {
                    "type": "number"
                  }
                }
              }
            },
            "required": [
              "matrix"
            ]
          },
          "userId": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "choices",
          "factors",
          "factorsChoices",
          "userId"
        ]
      }
    }
